<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jindam.app.common.mapper.HistoryLogMapper">

    <insert id="insertErrorLog" parameterType="HistoryLogRequestDto">
        INSERT INTO tb_com_log_error (
            request_info,
            create_id,
            create_at
        )
        VALUES (
            #{requestInfo},
            #{createId},
            #{createAt}
        )
    </insert>

</mapper> 